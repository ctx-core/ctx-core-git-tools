{
  "name": "@ctx-core/ctx-core-git-tools",
  "version": "5.3.3",
  "description": "git tools for the @ctx-core monorepo",
  "keywords": [
    "ctx-core"
  ],
  "homepage": "https://github.com/ctx-core/ctx-core-git-tools#readme",
  "bugs": {
    "url": "https://github.com/ctx-core/ctx-core-git-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ctx-core/ctx-core-git-tools/issues"
  },
  "license": "Apache-2.0",
  "author": "Brian Takita",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
    "compile": "./node_modules/.bin/tsc -b .",
    "exec": "$@"
  },
  "devDependencies": {
    "typescript": "^4.3.2"
  },
  "publishConfig": {
    "access": "public",
    "cache": "~/.npm"
  }
}
